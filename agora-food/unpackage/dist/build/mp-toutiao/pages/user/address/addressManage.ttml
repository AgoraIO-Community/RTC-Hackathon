<view class="content"><hx-navbar vue-id="1" title="收货地址" backTabbarUrl="/pages/user/center/center" backgroundColor="{{[241,241,241]}}" bind:__l="__l"></hx-navbar><view class="bb15"></view><view class="list-box"><view class="row b-b"><text class="tit">联系人</text><input class="input" type="text" placeholder="收货人姓名" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['addressData']]]]]}}" value="{{addressData.name}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">手机号</text><input class="input" type="number" placeholder="收货人手机号码" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','mobile','$event',[]],['addressData']]]]]}}" value="{{addressData.mobile}}" bindinput="__e"/></view><view class="row b-b"><text class="tit">地址</text><text data-event-opts="{{[['tap',[['chooseLocation',['$event']]]]]}}" class="input" bindtap="__e">{{''+addressData.addressName+''}}</text><text class="hxicon-locationfill"></text></view><view class="row b-b"><text class="tit">门牌号</text><input class="input" type="text" placeholder="楼号、门牌" placeholder-class="placeholder" data-event-opts="{{[['input',[['__set_model',['$0','area','$event',[]],['addressData']]]]]}}" value="{{addressData.area}}" bindinput="__e"/></view><view class="row default-row"><text class="tit">设为默认</text><switch checked="{{addressData.defaule}}" color="#ffc107" data-event-opts="{{[['change',[['switchChange',['$event']]]]]}}" bindchange="__e"></switch></view></view><view data-event-opts="{{[['tap',[['confirm',['$event']]]]]}}" class="add-btn" bindtap="__e"><text>提交</text></view></view>