/*边框*/
.b-b{
 	position: relative;
}
.b-b:after,
 .b-t:after {
 	position: absolute;
 	z-index: 3;
 	left: 0;
 	right: 0;
 	height: 0;
 	content: '';
 	-webkit-transform: scaleY(.5);
 	        transform: scaleY(.5);
 	border-bottom: 1px solid #E4E7ED;
}
.b-b:after {
 	bottom: 0;
}
.b-t:after {
 	top: 0;
}
