.bb15 {
  padding-bottom: 30rpx;
}
.content {
  position: relative;
}
.list-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  background-color: #fff;
  border-radius: 10px;
  overflow: hidden;
  margin: 0 15px;
}
.list {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 14px 15px;
  background: #fff;
  position: relative;
}
.wrapper {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.address-box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.address-box .tag {
  font-size: 24rpx;
  color: #ffc107;
  margin-right: 10rpx;
  background: #fffafb;
  border: 1px solid #ffc107;
  border-radius: 4rpx;
  padding: 4rpx 10rpx;
  line-height: 1;
}
.address-box .address {
  font-size: 30rpx;
  color: #303133;
}
.u-box {
  font-size: 28rpx;
  color: #909399;
  margin-top: 16rpx;
}
.u-box .name {
  margin-right: 30rpx;
}
.icon-edit {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  height: 80rpx;
  font-size: 40rpx;
  color: #909399;
  padding-left: 30rpx;
}
.add-btn {
  position: fixed;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  border-radius: 10px;
  overflow: hidden;
  bottom: 15px;
  left: 15px;
  right: 15px;
  height: 45px;
  font-size: 16px;
  font-weight: bold;
  color: #333;
  background: -webkit-linear-gradient(45deg, #ffd900, #ffc107);
  background: linear-gradient(45deg, #ffd900, #ffc107);
}

