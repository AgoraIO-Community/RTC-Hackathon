<view><hx-navbar vue-id="679c3432-1" title="支付订单" backgroundColor="{{[241,241,241]}}" bind:__l="__l"></hx-navbar><view class="head"><view class="row"><block tt:if="{{countdownStr}}"><text>{{"支付剩余时间 "+countdownStr}}</text></block><block tt:else><text>重新提交订单</text></block></view><view class="row pricebox pt-8"><text class="fh">￥</text><text class="txt1">{{orderInfo.money}}</text></view><view data-event-opts="{{[['tap',[['doOrder',['$event']]]]]}}" class="row pt-4" bindtap="__e"><text>{{orderInfo.store_name}}</text><block tt:if="{{orderInfo.store_community}}"><text>{{"（"+orderInfo.store_community+"）"}}</text></block><text>订单详情</text><view class="hxicon-right _i"></view></view></view><view class="container"><radio-group data-event-opts="{{[['change',[['payChange',['$event']]]]]}}" class="list-box" bindchange="__e"><block tt:for="{{payData}}" tt:for-item="item" tt:for-index="i" tt:key="i"><block><view class="flex-rl list-item"><view class="left"><view class="iconbox" style="{{('background-color:'+item.icon_color)}}"><view class="{{(('_i')+' '+item.icon)}}"></view></view></view><view class="b-b right"><view class="tit-box"><text class="tit">{{item.name}}</text></view><radio value="{{item.id}}" color="#ffc107" checked="{{item.default==1}}"></radio></view></view></block></block></radio-group></view><view data-event-opts="{{[['tap',[['submitPay',['$event']]]]]}}" class="btn" bindtap="__e"><text>确认支付</text></view></view>